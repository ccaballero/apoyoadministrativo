{% extends '::base.html.twig' %}

{% block breadcrumb %}
<ol class="breadcrumb">
    <li><a href="{{ path(url_list) }}">{{ title_list }}</a></li>
    <li><span>{{ title_create }}</span></li>
</ol>
{% endblock %}

{% block body %}
<section>
    <div class="container">
        <h1>{{ title_create }}</h1>
        {{ form(form) }}
    </div>
</section>
<script>$(document).ready(function(){$('input[type="text"]:first').focus()})</script>
{% endblock %}
