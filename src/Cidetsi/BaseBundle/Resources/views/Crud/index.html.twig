{% extends '::base.html.twig' %}

{% block breadcrumb %}
<ol class="breadcrumb">
    <li><span>{{ title_list }}</span></li>
</ol>
{% endblock %}

{% block body %}
<section>
    <div class="container">
        {{ include('CidetsiBaseBundle:Block:flashbag.html.twig') }}
        {% block preamble %}
            <h1>{{ title_list }}</h1>
        {% endblock %}
        {% for entity in entities %}
            {% block items %}{% endblock %}
        {% else %}
            <p>No se encontraron entidades.</p>
        {% endfor %}
    </div>
</section>
{% endblock %}
