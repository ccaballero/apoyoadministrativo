<div class="btn-toolbar">
    <div class="btn-group">
    {% if view %}
        <a class="btn btn-primary"
           href="{{ path('docente_read', {
            'id': entity.slug }) }}">Ver detalles</a>
    {% endif %}
    {% if edit %}
        <a class="btn btn-default"
           href="{{ path('docente_update_get', {
            'id': entity.slug }) }}">Editar</a>
    {% endif %}
    {% if delete %}
        <a class="btn btn-default"
           href="{{ path('docente_delete_get', {
            'id': entity.slug }) }}">Eliminar</a>
    {% endif %}
    </div>
</div>
