{% extends 'CidetsiBaseBundle:Crud:index.table.html.twig' %}

{% block preamble %}
    {{ parent() }}
    {{ include('CidetsiDocentesBundle:Docente:Tasks/collection.html.twig') }}
{% endblock %}

{% block headers %}
    <tr>
        <th>Apellido Paterno</th>
        <th>Apellido Materno</th>
        <th>Nombres</th>
        <th>&nbsp;</th>
    </tr>
{% endblock %}

{% block items %}
<tr>
    <td>{{ entity.apellidoPaterno }}</td>
    <td>{{ entity.apellidoMaterno }}</td>
    <td>{{ entity.nombres }}</td>
    <td class="text-center">
        <a href="{{ path('docente_read', {'id': entity.ident}) }}"
           alt="Ver" title="Ver">
           <span class="glyphicon glyphicon-search"></span></a>&nbsp;
        <a href="{{ path('docente_update_get', {'id': entity.ident}) }}"
           alt="Editar" title="Editar">
           <span class="glyphicon glyphicon-pencil"></span></a>&nbsp;
        <a href="{{ path('docente_delete_get', {'id': entity.ident}) }}"
           alt="Eliminar" title="Eliminar">
           <span class="glyphicon glyphicon-remove"></span></a>
    </td>
</tr>
{% endblock %}

