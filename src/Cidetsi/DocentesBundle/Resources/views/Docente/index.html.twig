{% extends 'CidetsiBaseBundle:Crud:index.html.twig' %}

{% block body %}
<section>
    <div class="container">
        {{ include('CidetsiBaseBundle:Block:flashbag.html.twig') }}
        <h1>{{ title_list }}</h1>
        {{ include('CidetsiDocentesBundle:Docente:Tasks/collection.html.twig') }}
        <div class="table-responsive">
            <table class="table table-striped table-condensed">
                <tr>
                    <th>Apellido Paterno</th>
                    <th>Apellido Materno</th>
                    <th>Nombres</th>
                </tr>
            {% for entity in entities %}
                <tr>
                    <td>{{ entity.apellidoPaterno }}</td>
                    <td>{{ entity.apellidoMaterno }}</td>
                    <td>{{ entity.nombres }}</td>
                </tr>
            {% endfor %}
            </table>
        </div>
    </div>
</section>
{% endblock %}
