<!DOCTYPE html>
<html>
    <head>
        <title>{% block title %}Apoyo administrativo{% endblock %}</title>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <link rel="icon" type="image/x-icon"
              href="{{ asset('/favicon.ico') }}" />
        {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset(
              '/bundles/cidetsibase/css/bootstrap/bootstrap.min.css') }}" />
        <link rel="stylesheet" href="{{ asset(
              '/bundles/cidetsibase/css/bootstrap/bootstrap-theme.css') }}" />
        <link rel="stylesheet" href="{{ asset(
              '/bundles/cidetsibase/css/bushido.css') }}" />
        {% endblock %}
        {% block scripts %}
        <script src="{{ asset(
                '/bundles/cidetsibase/js/jquery.min.js') }}"></script>
        <script src="{{ asset(
                '/bundles/cidetsibase/js/bootstrap.min.js') }}"></script>
        <script src="{{ asset(
                '/bundles/cidetsibase/js/bushido.js') }}"></script>
        {% endblock %}
    </head>
    <body>
        <header class="navbar navbar-inverse navbar-fixed-top" role="banner">
            <div class="container">
                {{ render(controller('CidetsiBaseBundle:Block:menu')) }}
            </div>
        </header>
        {% block breadcrumb %}{% endblock %}
        {% block body %}{% endblock %}
        <footer class="container">
            {{ render(controller('CidetsiBaseBundle:Block:footer')) }}
        </footer>
    </body>
</html>
