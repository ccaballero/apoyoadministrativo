<!DOCTYPE html>
<html>
    <head>
        <title>{% block title %}Apoyo administrativo{% endblock %}</title>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <link rel="icon" type="image/x-icon"
              href="{{ asset('/favicon.ico') }}" />
        {% block stylesheets %}
        <link rel="stylesheet"
              href="{{ asset(
              '/bundles/cidetsibase/css/bootstrap/bootstrap.min.css') }}" />
        {% endblock %}
        {% block scripts %}
        <script src="{{
            asset('/bundles/cidetsibase/js/jquery.min.js') }}"></script>
        <script src="{{
            asset('/bundles/cidetsibase/js/bootstrap.min.js') }}"></script>
        {% endblock %}
    </head>
    <body>
        <header>
            <nav class="navbar navbar-inverse navbar-fixed-top"
                 role="navigation">
                <div class="container">
                    {{ render(controller('CidetsiBaseBundle:Default:menu')) }}
                </div>
            </nav>
        </header>
        {% block body %}{% endblock %}
        <footer>
            <nav>
                {{ render(controller('CidetsiBaseBundle:Default:footer')) }}
            </nav>
        </footer>
    </body>
</html>
